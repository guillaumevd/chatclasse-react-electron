{
  "name": "client",
  "preductname": "Chat Classe",
  "version": "0.2.3",
  "description": "Descripton de l'appli",
  "author": {
    "name": "Guillaume Vandriessche",
    "email": "Guillaumevandriessche776@gmail.com"
  },
  "private": true,
  "main": "main_process/app.js",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumevd/chatclasse-react-electron.git"
  },
  "scripts": {
    "build": "rimraf build dist && yarn version --patch && react-scripts build && node build.js",
    "serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "start": "wait-on tcp:127.0.0.1:3000 && cross-env-shell NODE_ENV=dev electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@electron/remote": "1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "add": "^2.0.6",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^5.3.0",
    "react": "^18.2.0",
    "react-chat-engine": "^1.11.28",
    "react-chat-engine-pretty": "^0.1.8",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.3",
    "electron-builder": "^23.6.0",
    "rimraf": "^4.4.0",
    "wait-on": "5.2.1"
  }
}
